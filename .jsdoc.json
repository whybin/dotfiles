{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js$",
        "excludePattern": "(node_modules|docs|target|flow|ext|tests)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": false
    }
}
